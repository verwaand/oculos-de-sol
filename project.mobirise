    "privacidade-e-termos.html": {
      "settings": {
        "meta_descr": "Política de privacidade e termos de uso.",
        "title": "Privacidade e Termos | VERWAAND",
        "order": 1
      },
      "components": [
        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "background-color": "@bgColor",
            ".mbr-text, blockquote": {
              "color": "#767676"
            },
